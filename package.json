{
  "name": "bluemath",
  "version": "0.1.1",
  "description": "Math kernel in Javascript",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "watch": "webpack --watch --config webpack.config.js",
    "watch-logoscope": "webpack --watch --config webpack.logoscope.config.js",
    "build-docs": "typedoc --excludeNotExported --out ./docs --theme doctheme/ --mode file --readme src/index.md --gaID \"UA-99857402-1\"",
    "prepublish": "(cd src; tsc -p .)"
  },
  "keywords": [
    "Math",
    "Linear Algebra",
    "Vector",
    "Matrix"
  ],
  "author": "Jayesh Salvi",
  "license": "AGPL-3.0",
  "devDependencies": {
    "@types/qunit": "^2.0.31",
    "awesome-typescript-loader": "^3.1.3",
    "blueplot": "^0.1.0",
    "qunitjs": "^2.3.2",
    "source-map-loader": "^0.2.1",
    "typedoc": "^0.7.1",
    "typescript": "^2.3.2",
    "webpack": "^2.5.1"
  },
  "dependencies": {}
}
